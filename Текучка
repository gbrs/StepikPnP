"""
Выведите таблицу размером n×n, заполненную числами от 1 до n^2 по спирали,
выходящей из левого верхнего угла и закрученной по часовой стрелке

"""


N = int(input())
arr = [[None for j in range(N)] for i in range(N)]

# границы уже обойденного поля
left = up = 0
right = down = N - 1
cnt = 1

while True:
    if left > right:
        break
    for column in range(left, right + 1):
        arr[up][column] = cnt
        cnt += 1
    up += 1

    if up > down:
        break
    for line in range(up, down + 1):
        arr[line][right] = cnt
        cnt += 1
    right -= 1

    if left > right:
        break
    for column in range(right, left - 1, -1):
        arr[down][column] = cnt
        cnt += 1
    down -= 1

    if up > down:
        break
    for line in range(down, up - 1, -1):
        arr[line][left] = cnt
        cnt += 1
    left += 1

[print(*i) for i in arr]

for i in range(N):
    print(*arr[i], sep='\t')
